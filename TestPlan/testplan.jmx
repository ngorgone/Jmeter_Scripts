        <boolProp name="resetInterpreter">false</boolProp>
        <stringProp name="script">import java.util.List;
import java.util.ArrayList;
import org.apache.commons.codec.digest.DigestUtils;

 /*************************** *********************************************************
  * 
  * Parse LOAD param and
  * set requests per second, warmup and duration.
  * 
  *************************************************************************************/

 	String idm_load = vars.get(&quot;load&quot;);
	String idm_am1 = null;
	String idm_id1 = null;
	String idm_id2 = null;

	if (idm_load.equals(&quot;normal&quot;)) {
        idm_am1 = &quot;1&quot;;
        idm_id1 = &quot;1&quot;;
        idm_id2 = &quot;1&quot;;
    } else if (idm_load.equals(&quot;peak&quot;)) {
        idm_am1 = &quot;5&quot;;
        idm_id1 = &quot;2&quot;;
        idm_id2 = &quot;3&quot;;
    } else if (idm_load.equals(&quot;load&quot;)) {
        idm_am1 = &quot;10&quot;;
        idm_id1 = &quot;4&quot;;
        idm_id2 = &quot;6&quot;;
    } else{
        log.error(&quot;WARNING - LOAD PARAMETER WAS NOT SPECIFIED. VALID VALUES ARE : (normal,peak,load)&quot;);
        log.error(&quot;WARNING - DEFAULTING TO NORMAL LOAD&quot;);
        idm_am1 = &quot;1&quot;;
        idm_id1 = &quot;1&quot;;
        idm_id2 = &quot;1&quot;;
    }
	
	vars.put(&quot;idm_am1&quot;,idm_am1);
	vars.put(&quot;idm_id1&quot;,idm_id1);
	vars.put(&quot;idm_id2&quot;,idm_id2);	</stringProp>
